  </div>

  <div class="form-group">
    <%= label_tag nil, @predio.class.human_attribute_name(:cd_bloco) %>
    <%= text_field_tag nil,@predio.cd_bloco , class:"form-control", disabled: true %>
  </div>
  
  <div class="form-group">
