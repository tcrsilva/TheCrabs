<table class="table table-hover">
  <thead>
    <tr>
      <th><%= @perfils.human_attribute_name(:nm_sindico) %></th>
      <th><%= @perfils.human_attribute_name(:cd_sindico) %></th>
      <th><%= @perfils.human_attribute_name(:ds_email) %></th>
